    </owl:Class>
    


    <!-- http://www.semanticweb.org/fan/ontologies/2016/7/untitled-ontology-26#肺动脉瓣 -->

    <owl:Class rdf:about="http://www.semanticweb.org/fan/ontologies/2016/7/untitled-ontology-26#肺动脉瓣">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/fan/ontologies/2016/7/untitled-ontology-26#肺动脉口结构"/>
        <rdfs:subClassOf>
            <owl:Restriction>
