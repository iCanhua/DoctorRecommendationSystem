    <!-- http://www.semanticweb.org/fan/ontologies/2016/7/untitled-ontology-26#继发性心肌病 -->

    <owl:Class rdf:about="http://www.semanticweb.org/fan/ontologies/2016/7/untitled-ontology-26#继发性心肌病">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/fan/ontologies/2016/7/untitled-ontology-26#心肌病"/>
        <rdfs:subClassOf>
