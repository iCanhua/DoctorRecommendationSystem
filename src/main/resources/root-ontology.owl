                <owl:onProperty rdf:resource="http://www.semanticweb.org/fan/ontologies/2016/7/untitled-ontology-26#是体征"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/fan/ontologies/2016/7/untitled-ontology-26#阵发性房室交界性心动过速"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/fan/ontologies/2016/7/untitled-ontology-26#是体征"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/fan/ontologies/2016/7/untitled-ontology-26#心房扑动"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/fan/ontologies/2016/7/untitled-ontology-26#是体征"/>
