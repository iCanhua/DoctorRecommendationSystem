    <owl:Class rdf:about="http://www.semanticweb.org/fan/ontologies/2016/7/untitled-ontology-26#实验室结果">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/fan/ontologies/2016/7/untitled-ontology-26#检查结果"/>
