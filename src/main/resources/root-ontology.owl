    <!-- http://www.semanticweb.org/fan/ontologies/2016/7/untitled-ontology-26#二尖瓣狭窄 -->

    <owl:Class rdf:about="http://www.semanticweb.org/fan/ontologies/2016/7/untitled-ontology-26#二尖瓣狭窄">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/fan/ontologies/2016/7/untitled-ontology-26#二尖瓣疾病"/>
        <rdfs:subClassOf>
            <owl:Restriction>
