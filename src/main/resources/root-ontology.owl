            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/fan/ontologies/2016/7/untitled-ontology-26#是部分"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/fan/ontologies/2016/7/untitled-ontology-26#房间隔"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/fan/ontologies/2016/7/untitled-ontology-26#卵圆窝结构 -->

    <owl:Class rdf:about="http://www.semanticweb.org/fan/ontologies/2016/7/untitled-ontology-26#卵圆窝结构">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/fan/ontologies/2016/7/untitled-ontology-26#房间隔结构"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/fan/ontologies/2016/7/untitled-ontology-26#压迫症状 -->

    <owl:Class rdf:about="http://www.semanticweb.org/fan/ontologies/2016/7/untitled-ontology-26#压迫症状">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/fan/ontologies/2016/7/untitled-ontology-26#周围血管疾病体征"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/fan/ontologies/2016/7/untitled-ontology-26#是体征"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/fan/ontologies/2016/7/untitled-ontology-26#周围动脉瘤"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/fan/ontologies/2016/7/untitled-ontology-26#原发孔缺损 -->

    <owl:Class rdf:about="http://www.semanticweb.org/fan/ontologies/2016/7/untitled-ontology-26#原发孔缺损">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/fan/ontologies/2016/7/untitled-ontology-26#房间隔缺损"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/fan/ontologies/2016/7/untitled-ontology-26#原发性二尖瓣脱垂 -->

