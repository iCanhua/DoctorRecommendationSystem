    <!-- http://www.semanticweb.org/fan/ontologies/2016/7/untitled-ontology-26#血尿 -->

    <owl:Class rdf:about="http://www.semanticweb.org/fan/ontologies/2016/7/untitled-ontology-26#血尿">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/fan/ontologies/2016/7/untitled-ontology-26#周围血管疾病体征"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/fan/ontologies/2016/7/untitled-ontology-26#心血管疾病症状"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/fan/ontologies/2016/7/untitled-ontology-26#按生殖部位分"/>
