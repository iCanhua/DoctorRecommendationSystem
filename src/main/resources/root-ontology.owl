                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/fan/ontologies/2016/7/untitled-ontology-26#全心衰竭"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/fan/ontologies/2016/7/untitled-ontology-26#导致"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/fan/ontologies/2016/7/untitled-ontology-26#预激综合征"/>
