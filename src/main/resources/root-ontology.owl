    <!-- http://www.semanticweb.org/fan/ontologies/2016/7/untitled-ontology-26#病毒性心肌炎 -->

    <owl:Class rdf:about="http://www.semanticweb.org/fan/ontologies/2016/7/untitled-ontology-26#病毒性心肌炎">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/fan/ontologies/2016/7/untitled-ontology-26#急性心肌炎"/>
        <rdfs:subClassOf>
            <owl:Restriction>
